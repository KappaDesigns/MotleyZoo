{
  "presets": [
    [
			"env",
      {
        "modules": false,
        "targets": {
          "browsers": ["last 2 versions"]
        }
      }
    ],
  ],
  "plugins": [
    "syntax-dynamic-import",
  ],
	"env": {
		"karma": {
			"plugins": [
				"istanbul",
				{
					"exclude": ["tests/*.test.js"]
				}
			]
		}
	}
}
